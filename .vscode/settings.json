{
    "lua.targetVersion": "5.2",
    "Lua.diagnostics.globals": [
        "global",
        "defines",
        "game",
        "script",
        "remote",
        "data",
        "bit32",
        "settings",
        "mods",
        "rendering",
        "commands",
        "log",
				"serpent"
    ],
    "Lua.runtime.version": "Lua 5.2",
    "Lua.workspace.ignoreDir": [
        ".vscode",
        ".github",
        "locale"
    ],
		"Lua.telemetry.enable": false,
		"Lua.runtime.builtin": {
			"table": "enable",
			"math": "enable",
			"string": "enable",
			"package": "enable",
			"debug": "enable",
			"io": "disable",
			"jit": "disable",
			"basic": "disable",
			"bit": "disable",
			"bit32": "enable",
			"builtin": "disable",
			"coroutine": "disable",
			"ffi": "disable",
			"os": "disable",
			"utf8": "disable"
		},
		"Lua.intelliSense.searchDepth": 3,
		"Lua.hint.enable": true,
		"Lua.diagnostics.workspaceDelay": 50,
		"git.autofetch": "all",
		"markdownlint.config": {
			"MD033": false,
			"MD034": false,
		}
}